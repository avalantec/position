<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>

      <record model="ir.ui.view" id="res_partner_form_customization">
          <field name="name">res.partner.form.customization</field>
          <field name="model">res.partner</field>
          <field name="inherit_id" ref="base.view_partner_form"/>
          <field name="priority">99</field>
          <field name="arch" type="xml">
              
            <xpath expr="//form[1]/sheet[1]/notebook[1]/page[1]/field[@name='child_ids']/form[1]/sheet[1]/group[1]/group[2]/field[@name='mobile']" position="after">
              <field name="vat"/>
            </xpath>  
              
          </field>
      </record>    
      
  </data>
</odoo>
